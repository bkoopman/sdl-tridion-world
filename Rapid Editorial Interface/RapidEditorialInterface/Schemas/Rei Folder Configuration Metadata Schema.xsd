<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tcmi="http://www.tridion.com/ContentManager/5.0/Instance" xmlns="http://www.sdl.com/SdlTridion/Schema/ReiConfig" targetNamespace="http://www.sdl.com/SdlTridion/Schema/ReiConfig" elementFormDefault="qualified">
  <xsd:annotation>
